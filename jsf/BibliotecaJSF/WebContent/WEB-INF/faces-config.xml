<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<application>
		<message-bundle>resources.application</message-bundle>
		<locale-config>
			<default-locale>es</default-locale>
			<supported-locale>en</supported-locale>
		</locale-config>
	</application>
	<managed-bean>
		<managed-bean-name>backingAutor</managed-bean-name>
		<managed-bean-class>backing.BackingAutor</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>backingSocio</managed-bean-name>
		<managed-bean-class>backing.BackingSocio</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>backingMoroso</managed-bean-name>
		<managed-bean-class>backing.BackingMoroso</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>backingLibro</managed-bean-name>
		<managed-bean-class>backing.BackingLibro</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

</faces-config>
